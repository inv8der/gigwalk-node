{
  "name": "gigwalk",
  "version": "0.0.0-semantic-release",
  "description": "Gigwalk API Client - universal.",
  "main": "./lib",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "NODE_ENV=production babel src -d lib -s --ignore 'src/**/*_test.js'",
    "postbuild": "gulp",
    "eslint": "eslint .",
    "flow": "flow",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test mocha 'src/**/*_test.js'",
    "test-api": "NODE_ENV=test mocha --require ./test/apiSetup.js --timeout 30000 ./test/api/",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gigwalk-corp/gigwalk-node.git"
  },
  "keywords": [
    "gigwalk",
    "api",
    "client",
    "axios"
  ],
  "author": "Marc Ryan <marc@gigwalk.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gigwalk-corp/gigwalk-node/issues"
  },
  "homepage": "https://github.com/gigwalk-corp/gigwalk-node#readme",
  "engineStrict": true,
  "engines": {
    "node": "8.9.1",
    "npm": ">=5.0.0 <6.0.0",
    "yarn": "^1.1.0"
  },
  "dependencies": {
    "ajv": "^5.1.5",
    "babel-runtime": "^6.26.0",
    "btoa": "^1.1.2",
    "deep-diff": "^0.3.8",
    "humps": "^2.0.1",
    "json-stable-stringify": "^1.0.1",
    "lodash.clonedeep": "^4.4.1",
    "lodash.isplainobject": "^4.0.6",
    "url-template": "^2.0.8"
  },
  "peerDependencies": {
    "axios": "0.15.x"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "axios-mock-adapter": "^1.7.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-rewire": "^1.0.0-rc-4",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-json-schema": "^1.5.0",
    "condition-circle": "^1.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-gigwalk": "^1.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.44.2",
    "gulp": "^3.9.1",
    "gulp-beautify": "^2.0.1",
    "gulp-eslint": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "json-schema-ref-parser": "^4.0.4",
    "mocha": "^4.0.1",
    "mockery": "^2.1.0",
    "moniker": "^0.1.2",
    "semantic-release": "^6.3.2",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "through2": "^2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
